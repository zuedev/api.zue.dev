{
  "$schema": "node_modules/wrangler/config-schema.json",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "compatibility_date": "2025-02-05",
  "name": "api-zue-dev",
  "main": "source/main.js",
  "routes": [
    {
      "pattern": "api.zue.dev",
      "custom_domain": true
    }
  ],
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  },
  "browser": {
    "binding": "MYBROWSER"
  }
}
